{
  "manifest_version": 3,
  "name": "Chill House",
  "version": "1.0",
  "description": "Påminnelser om att dricka vatten, äta lunch och ta fika. 💧🍽☕",
  "permissions": ["alarms", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
